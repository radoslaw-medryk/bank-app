{
    "name": "@radoslaw-medryk/bank-app",
    "private": true,
    "scripts": {
        "build-dev": "mkdir -p dist && rm -r dist && npm run prettier-check && tsc && webpack --mode=production && cp configs/dev-config.json dist/config.json",
        "build-prod": "mkdir -p dist && rm -r dist && npm run prettier-check && tsc && webpack --mode=production && cp configs/prod-config.json dist/config.json",
        "deploy-prod": "npm run build-prod && npx firebase deploy --project bank-proj",
        "deploy-dev": "npm run build-dev && npx firebase deploy --project dev-bank-proj",
        "start": "webpack-dev-server",
        "lint:css": "stylelint src/**/*.tsx",
        "prettier": "prettier ./src/**/*.{js,ts,tsx,json,html} --write",
        "prettier-check": "prettier ./src/**/*.{js,ts,tsx,json,html} --check"
    },
